	<xs:complexType name="QboBillingCodeStatus">
		<xs:annotation>
			<xs:documentation>Defines status of the billing code</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Active" type="xs:boolean" minOccurs="0">
