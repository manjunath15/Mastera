	<xs:complexType name="QboSKU">
		<xs:annotation>
			<xs:documentation>Defines QBO Company's SKU (e.g. QBO Plus, QBO Essentials, etc.)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ID" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">SKU ID (e.g. 12)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">SKU Long Name (e.g. QuickBooks Online Essentials)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QboBillingCodePayroll">
		<xs:annotation>
			<xs:documentation>Defines Payroll offer associated with QBO Billing Code</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Disclaimer" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">monthly price</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QboBillingCodePrice">
		<xs:annotation>
			<xs:documentation>Defines monthly and annual price of a QBO Billing Code</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Monthly" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">monthly price</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Yearly" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">yearly price</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Billing Currency</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencySymbol" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Billing Currency Symbol</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="QboBillingCodeDiscount">
		<xs:annotation>
			<xs:documentation>Defines discount for the billing code</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DiscountAmount" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Amount of Fixed Amount Discount,
                        Percentage Discount, etc. Look at IntroductoryDiscountType.enum
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DiscountTypeID" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Discount Type ID.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DiscountTypeDesc" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Description of discount type
                        e.g. fixed amount discount, percentage discount, etc.
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DiscountDurationUnitType" type="xs:int" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Duration unit type. 0 is day and 1 is month.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DiscountDurationUnitTypeDescription" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Duration unit type. day or month.</xs:documentation>
                </xs:annotation>
            </xs:element>
			<xs:element name="DiscountOfferLength" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Discount offer length</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DiscountDuration" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Discount duration</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
